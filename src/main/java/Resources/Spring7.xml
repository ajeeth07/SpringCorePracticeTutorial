<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
			"https://www.springframework.org/dtd/spring-beans-2.0.dtd">
	<!-- Values injected in order -->		
	<beans>
	<bean id="S1" class="Constructor_PrimitiveDI.Student">
	<constructor-arg value="709"></constructor-arg>
	<constructor-arg value="Ajeeth"></constructor-arg>
	<constructor-arg value="85"></constructor-arg>
	</bean>
	
	<!-- Type attribute -->
	<bean id="ST1" class="Constructor_PrimitiveDI.Student1_TypeAttribute">
	<constructor-arg value="800" type="int"></constructor-arg>
	<constructor-arg value="Vijay" type="String"></constructor-arg>
	<constructor-arg value="86" type="double"></constructor-arg>
	</bean>
	
	<!-- index Attribute  -->
	<bean id="SIT1" class="Constructor_PrimitiveDI.Student2_indexAttribute">
	<constructor-arg value="800" index="0"></constructor-arg>
	<constructor-arg value="86" index="2"></constructor-arg>
	<constructor-arg value="Vijay" index="1"></constructor-arg>
	
	</bean>
	
	
	
	
	</beans>